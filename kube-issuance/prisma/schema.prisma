datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Credential {
  id         String   @id @default(cuid())
  title      String
  subject    String
  payload    String
  hash       String   @unique
  issuedBy   String
  issuedAt   DateTime @default(now())
  expiryDate DateTime?
  workerId   String
}